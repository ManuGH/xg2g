{
  "name": "webui",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "generate-client": "openapi-ts --input ../api/openapi.yaml --output ./src/client-ts",
    "generate-client:legacy": "openapi --input ../api/openapi.yaml --output ./src/client-legacy --client fetch",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "hls.js": "^1.6.15",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "plyr": "^3.8.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@hey-api/client-fetch": "^0.13.1",
    "@hey-api/openapi-ts": "^0.89.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "openapi-typescript": "^7.10.1",
    "openapi-typescript-codegen": "^0.30.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  }
}