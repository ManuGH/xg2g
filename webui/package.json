{
  "name": "webui",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "generate-client": "openapi-ts --input ../api/openapi.yaml --output ./src/client-ts",
    "generate-client:legacy": "openapi --input ../api/openapi.yaml --output ./src/client-legacy --client fetch",
    "lint": "eslint . && bash ../scripts/ci_gate_webui_logging.sh",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.20",
    "hls.js": "^1.6.15",
    "i18next": "^25.8.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "lodash": "^4.17.23",
    "plyr": "^3.8.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@hey-api/client-fetch": "^0.13.1",
    "@hey-api/openapi-ts": "^0.91.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "fast-check": "^4.5.3",
    "globals": "^17.3.0",
    "jsdom": "^28.0.0",
    "openapi-typescript": "^7.10.1",
    "openapi-typescript-codegen": "^0.30.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  }
}
