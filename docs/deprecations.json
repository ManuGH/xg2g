{
  "version": 1,
  "deprecations": [
    {
      "id": "stream-port-env",
      "item": "XG2G_STREAM_PORT",
      "key": "XG2G_STREAM_PORT",
      "target": "enigma2.streamPort",
      "phase": "warn",
      "replacement": "enigma2.streamPort",
      "remove_in": "v3.5.0",
      "message": "Use YAML config for streaming ports; ENV override will be removed in v3.5 (ADR-00X)"
    },
    {
      "id": "http2-toggle",
      "item": "XG2G_HTTP_ENABLE_HTTP2",
      "key": "XG2G_HTTP_ENABLE_HTTP2",
      "phase": "fail",
      "replacement": "Always Enabled",
      "remove_in": "v3.1.0",
      "message": "HTTP/2 toggle removed. HTTP/2 is now always enabled for V3. Remove from ENV."
    }
  ]
}