# xg2g Secrets Template
# Copy this file to secrets.yaml and add your actual credentials
# DO NOT commit secrets.yaml to git!
apiVersion: v1
kind: Secret
metadata:
  name: xg2g-secrets
  namespace: xg2g
type: Opaque
stringData:
  # OpenWebIF Credentials
  owi-username: ""  # Your OpenWebIF username
  owi-password: ""  # Your OpenWebIF password

  # API Token (optional)
  api-token: ""  # Your API token for authentication

  # Redis Password (if using authenticated Redis)
  redis-password: ""
